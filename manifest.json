{
  "manifest_version": 2,
  "name": "",
  "description": "Clear browsing history by keywords.",
  "version": "1.0",
  "browser_action": {
    "default_title": ""
  },
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "history",
    "storage"
  ]
}
